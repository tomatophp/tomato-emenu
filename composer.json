{
    "name": "tomatophp/tomato-emenu",
    "type": "library",
    "description": "Tomato E-Menu To Manage Menus for Tomato Ecommerce",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoEmenu\\": "src/"
        }
    },
    "autoload-dev": {
